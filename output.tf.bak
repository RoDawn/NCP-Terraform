terraform {
  required_version = ">= 0.13"
  required_providers {
    ncloud = {
      source = "NaverCloudPlatform/ncloud"
    }
    null = {
      source = "hashicorp/null"
    }
    random = {
      source = "hashicorp/random"
  }
 }
}
provider "ncloud" {
  # access_key = "aaaaaaaaaaaaa"
 # secret_key = "bbbb"
  region = "KR"
  site = "public"
  support_vpc = "true"
}
